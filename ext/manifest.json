{
  "name": "Outside Sound",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Step outside your sound; take the edge off. Applies a lowpass filter to your audio to make it sound like you have left the room.",
  "homepage_url": "https://alexhoward.io",
  "icons": {
    "128": "icons/icon128-outside.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128-inside.png",
    "default_title": "Step outside ->"
  },
  "background": {
    "scripts": ["src/background.js"]
  },
  "permissions": ["tabs", "https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["src/inject.js"]
    }
  ]
}
